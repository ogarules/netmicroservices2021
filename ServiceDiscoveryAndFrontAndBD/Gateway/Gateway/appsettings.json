{
    "Logging": {
        "LogLevel": {
            "Default": "Information",
            "Microsoft": "Warning",
            "Microsoft.Hosting.Lifetime": "Information"
        }
    },
    "AllowedHosts": "*",
    "Spring": {
        "Application": {
            "Name": "app-gateway"
        }
    },
    "Eureka": {
        "Client": {
            "serviceUrl": "${HR_EUREKA}",
            "ShouldFetchRegistry": true,
            "ValidateCertificates": false,
            "ShouldRegisterWithEureka": false
        },
        "Instance": {
            "SecurePortEnabled": true
        }
    }
}